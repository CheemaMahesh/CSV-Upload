<html>
    <head>
<link rel="stylesheet" href="/css/csv.css">
</head>
<body>
<div class="nav-bar" ><div class="home"><a href="/">Home</a></div>
<!-- ==========================================================Search-Bar-=============================================================== -->
<div class="search-bar">
    <form id="searchForm">
        <div class="search-box">
            <input type="text" id="input" placeholder="Search Here...">
            <button id="search" class="search-btn" type="button">Search</button>
        </div>
    </form>
</div>

</div>

<!-- ===================================================CSV-Content-Display================================================================== -->
<div>
    <table>
        <thead>
            <tr>
                <% for(let h of head) { %>
                    <th class="headers">
                        <%= h %>
                    </th>
                    <% } %>
            </tr>
        </thead>
        <tbody>
            <% for(let row of data) { %>
                <tr>
                    <% for(let key of Object.keys(row)) { %>
                        <td>
                            <%= row[key] %>
                        </td>
                        <% } %>
                </tr>
                <% } %>
        </tbody>
    </table>
</div>
<script src="/js/csv.js"></script>
</body>

</html>